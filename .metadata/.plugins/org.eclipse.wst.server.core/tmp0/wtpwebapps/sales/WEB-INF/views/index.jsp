	<script type="text/javascript">
	function saveReminderDetails() {
	    
	    if (!validateZoneDetailsForm()) {
				return;
			}
	    
	    var formData = $("#requestdemoForm").serializeArray();
	    var loginFormObject = {};
	    
	    // Map form data to an object
	    $.each(formData, function(i, v) {
	        loginFormObject[v.name] = v.value;
	    });

	    // Make AJAX request
	    $.ajax({
	        url: "saveApp", // Your controller URL
	        type: "POST",
	        contentType: "application/json", // Sending JSON data
	        data: JSON.stringify(loginFormObject), // Serialize form data to JSON
	        success: function(result) {
	          showNotificationModal('COMPANY REGISTERED SUCCESSFULLY');
	        },
	        error: function(xhr) { 
	        	if (xhr.status === 409) {
                // Conflict error, email already registered
                showNotificationModal('COMPANY IS ALREADY REGISTERED');
            } else {
